:import factory constants

:local double previous_amount
:local int tier

tier = 0
tierloop:
tier = tier + 1

previous_amount = count("producer.statueofcubos", tier)
craft("producer.statueofcubos", tier, {get_queue(tier, black_producer)})
{subtract_queue(tier, black_producer, count("producer.statueofcubos", tier) - previous_amount)}


gotoif(tierloop, tier < 5)
