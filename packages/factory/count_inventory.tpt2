:import factory constants

:global int _tier

_tier = 1
loop:

executesync("count producers")
stop("count producers")
executesync("count machines")
stop("count machines")
executesync("count ingredients")
stop("count ingredients")

global.double.set("craft_queue_" . (_tier * tier_factor + ore), 0.)
global.double.set("craft_queue_" . (_tier * tier_factor + lump), 0.)

_tier += 1
gotoif(loop, _tier < 11)
global.double.set("craft_queue_" . (1 * tier_factor + hammer), 0. - count("hammer", 1))
global.double.set("craft_queue_" . (1 * tier_factor + rubber), 0. - count("rubber", 1))
global.double.set("craft_queue_" . (1 * tier_factor + rubber_plate), 0. - count("plate.rubber", 1))
global.double.set("craft_queue_" . (1 * tier_factor + exotic_producer), 0. - count("producer.exoticgems", 1))
global.double.set("craft_queue_" . (1 * tier_factor + gem_producer), 0. - count("producer.gems", 1))
